package bai05;

public class Card {

	private String face;
	private String number;

	public Card() {
		// TODO Auto-generated constructor stub
	}

	public Card(String face, String number) {
		super();
		this.face = face;
		this.number = number;
	}

	public String getface() {
		return face;
	}

	public void setface(String face) {
		this.face = face;
	}

	public String getnumber() {
		return number;
	}

	public void setnumber(String number) {
		this.number = number;
	}

}
