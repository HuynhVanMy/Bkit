package ex01;

import java.io.File;
import java.io.IOException;

public class LFile {
	public static void main(String[] args) {
		//create a new file
		File file = new File("test.txt");
		try {
			file.createNewFile();
			System.out.println("Sucess");
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
