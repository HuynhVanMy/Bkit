package bai02;

public class QuanLyCuaHangCD {

	private int maCD;
	private String chuDeCD;
	private String tenCasy;
	private int soBaiHat;
	private double giaThanh;

	public QuanLyCuaHangCD() {

	}

	public QuanLyCuaHangCD(int maCD, String chuDeCD, String tenCasy, int soBaiHat, double giaThanh) {
		super();
		this.maCD = maCD;
		this.chuDeCD = chuDeCD;
		this.tenCasy = tenCasy;
		this.soBaiHat = soBaiHat;
		this.giaThanh = giaThanh;
	}

}
