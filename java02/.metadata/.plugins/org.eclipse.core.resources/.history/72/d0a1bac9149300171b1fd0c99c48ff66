package bai04;

import java.util.Set;
import java.util.TreeMap;

public class DictionaryView {

	public static void main(String[] args) {
		TreeMap<String, Dictionary> dictionary = new TreeMap<>();
		dictionary.put("hello", new Dictionary("xin chào", "tính từ", ":))"));
		Set<String> diction = dictionary.keySet();
		for (String item : diction) {
			System.out.println(item + dictionary.get(item));
		}
	}

}
