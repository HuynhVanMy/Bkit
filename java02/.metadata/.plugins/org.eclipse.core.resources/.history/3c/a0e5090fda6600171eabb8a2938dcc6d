package bean;

public class Main {
	public static void main(String[] args) {
		Rectangle item = new Rectangle();
		
		item.setWith(20);
		item.setHeight(30);
		System.out.println(item.getWith());
		System.out.println(item.getHeight());
		System.out.println(item);
		System.out.println();
		System.out.println(item.area(item.getWith(), item.getHeight()));
	}
}
