package bai02;

public class QuanLyCuaHangCD {

	private int maCD;
	private String chuDeCD;
	private String tenCasy;
	private int soBaiHat;
	private double giaThanh;

	public QuanLyCuaHangCD() {

	}

	public QuanLyCuaHangCD(int maCD, String chuDeCD, String tenCasy, int soBaiHat, double giaThanh) {
		super();
		this.maCD = maCD;
		this.chuDeCD = chuDeCD;
		this.tenCasy = tenCasy;
		this.soBaiHat = soBaiHat;
		this.giaThanh = giaThanh;
	}

	public int getMaCD() {
		return maCD;
	}

	public void setMaCD(int maCD) {
		this.maCD = maCD;
	}

	public String getChuDeCD() {
		return chuDeCD;
	}

	public void setChuDeCD(String chuDeCD) {
		this.chuDeCD = chuDeCD;
	}

	public String getTenCasy() {
		return tenCasy;
	}

	public void setTenCasy(String tenCasy) {
		this.tenCasy = tenCasy;
	}

	public int getSoBaiHat() {
		return soBaiHat;
	}

	public void setSoBaiHat(int soBaiHat) {
		this.soBaiHat = soBaiHat;
	}

	public double getGiaThanh() {
		return giaThanh;
	}

	public void setGiaThanh(double giaThanh) {
		this.giaThanh = giaThanh;
	}

}
