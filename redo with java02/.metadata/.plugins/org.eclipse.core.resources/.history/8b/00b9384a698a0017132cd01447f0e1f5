package bean;

public class SinhVien {

	private int maSV;
	private String hoTen;
	private float diemLT;
	private float diemTH;

	public SinhVien() {

	}

	public SinhVien(int maSV, String hoTen, float diemLT, float diemTH) {
		this.maSV = maSV;
		this.hoTen = hoTen;
		this.diemLT = diemLT;
		this.diemTH = diemTH;
	}

}
